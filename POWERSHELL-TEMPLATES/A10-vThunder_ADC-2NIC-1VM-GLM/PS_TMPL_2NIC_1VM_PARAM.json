{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "value": "vth-user"
    },
    "adminPassword": {
      "value": "vth-Password"
    },
    "dnsLabelPrefix": {
      "value": "vth-inst1"
    },
    "virtual_network": {
      "value": "vth-vnet"
    },
    "vmName": {
      "value": "vth-inst1"
    },
    "vmSize": {
      "value": "Standard_DS2_v2"
    },
    "vThunderImage": {
      "value":"vthunder-adc-521-byol"
    },
    "publisherName": {
      "value": "a10networks"
    },
    "productName": {
      "value": "a10-vthunder-adc-521"
    },
    "nic1Name": {
      "value": "vth-inst1-mgmt-nic1"
    },
    "nic2Name": {
      "value": "vth-inst1-data-nic2"
    },
    "addressPrefixValue": {
      "value": "10.0.0.0/16"
    },
    "mgmtIntfPrivatePrefix": {
      "value": "10.0.1.0/24"
    },
    "mgmtIntfPrivateAddress": {
      "value": "10.0.1.5",
      "metadata": {
        "description": "vThunder by default takes mgmtIntPraivateAddress value using DHCP, to use this IP manual configuration is required."
      }
    },
    "eth1PrivatePrefix": {
      "value": "10.0.2.0/24"
    },
    "eth1PrivateAddress": {
      "value": "10.0.2.5"
    },
    "publicIPAddressName": {
      "value": "vth-inst1-mgmt-nic1-ip"
    },
    "networkSecurityGroupName": {
      "value": "vth-nsg"
    }
  }
}
